        "def factorial(n):\n",
        "    if n == 0:\n",
        "        return 1\n",
        "    else:\n",
        "        res = 1\n",
        "        for i in range(1, n + 1):\n",
        "            res *= i\n",
        "        return res\n",
        "\n",
        "# Example usage:\n",
        "number = 5\n",
        "print(f\"The factorial of {number} is {factorial(number)}\")\n"
      ],
